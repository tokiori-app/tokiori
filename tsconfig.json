{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "es2020",
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@hooks/*": ["hooks/*"],
      "@screens/*": ["screens/*"],
      "@types/*": ["types/*"],
      "@utils/*": ["utils/*"],
      "@constant/*": ["constant/*"]
    }
  }
}
